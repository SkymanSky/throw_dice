from die import Die

#D6 oluştur.

die=Die()

#Bazı atışlar yap ve sonucu bir listede sakla.
results=[]
for roll_num in range(1000):
    result=die.roll()
    results.append(result)

#Sonuçları analiz et.
frequencies=[]

for value in range(1,die.num_sides+1):
    frequency=results.count(value)
    frequencies.append(frequency)

print(frequencies)
